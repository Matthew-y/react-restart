    "compilerOptions": {
        "target": "es6",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "noEmit": true,
        "allowJs": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
        }

    }
}